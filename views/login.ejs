  <% include partials/header.ejs %>

  <h1><%= title %></h1>
  <div class="container">
 
 
      <div class="text-center d-sm-flex flex-sm-row justify-content-center">
        <!-- Login form -->
        <form class="form-signin" method="POST" autocomplete="off">
            <img class="mb-4" src="images/logo2.png" width="100">
            <h1 class = "h3 mb-3 font-weight-normal">Please enter Login Information</h1>
            
            <label for="inputUsername" class="sr-only">Username</label>
            <input type="text" 
              id="username" 
              class="form-control" 
              placeholder="Enter Username" 
              required 
              autofocus>
            <label for="inputPassword" class="sr-only">Password</label>
            <input type="password" 
              id="inputPassword" 
              class="form-control" 
              placeholder="Enter Password" 
              required>
            <input type="submit" class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
        </form>

      </div>
  
        
  </div>
  <% include partials/footer.ejs %>
  